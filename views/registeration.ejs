<%- include('./partials/header') %>
          <body class="d-flex flex-column">
            <div class="login">
              
              <main class="flex-shrink-0">
                  <!-- Navigation-->
                  <nav class="navbar navbar-expand-lg navbar-light bg-white py-3">
                      <div class="container px-5">
                          <a class="navbar-brand" href="/"><span class="fw-bolder text-primary">CAP Advisor</span></a>  
                      </div>
                  </nav>
                  <!-- Page content-->
                  <section class="py-5">
                      <div class="container px-5">
                          <!-- Contact form-->
                          <div class="bg-light rounded-4 py-5 px-4 px-md-5">
                              <div class="text-center mb-5">
                                  <div class="feature bg-primary bg-gradient-primary-to-secondary text-white rounded-3 mb-3"><i class="bi bi-envelope"></i></div>
                                  <h1 class="fw-bolder">Create Account</h1>
                                  <p class="lead fw-normal text-muted mb-0">About You</p>
                              </div>
                              <div class="row gx-5 justify-content-center">
                                  <div class="col-lg-8 col-xl-6">
                                    <% if (Object.keys(errors).length> 0) { %>
                                      <p style="color: red">Please correct the following errors:</p>
                                      <ul>
                                        <% Object.values(errors).forEach(function(errorMessage) { %>
                                          <li>
                                            <%= errorMessage %>
                                          </li>
                                          <% }); %>
                                      </ul>
                                      <% } %>
                                          <form  class="form-control-static" action="/registeration" method="POST" id="contactForm">
                                              <div  class="form-floating mb-3">
                                                  <label for="exampleInputEmail1">Email address</label>
                                                  <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                                                    placeholder="Enter email" required>
                                                </div>
      
                                                <div class="form-floating mb-3">
                                                  <label for="exampleInputPassword1">Password</label>
                                                  <input name="password" type="password" class="form-control" id="exampleInputPassword1"
                                                    placeholder="Password" required>
                                                </div>
      
                                                <div class="form-floating mb-3">
                                                  <label for="exampleInputPassword1">Confirm Password</label>
                                                  <input name="c_password" type="password" class="form-control" id="exampleInputPassword1"
                                                    placeholder="Password" required>
                                                </div>
      
                                                <div class="row form-floating mb-3">
                                                  <div class="col">
                                                    <input name="full_name" type="text" class="form-control" placeholder="First name(Arabic only)" required>
                                                  </div>
                                                  <div class="col">
                                                    <input name="student_id" type="text" class="form-control" placeholder="Student id(#########)" required>
                                                  </div>
                                                </div>
                                          
                                          <br>
                                          <p>Check which box apply to you:</p>
                                          
                                          <div class="form-check form-check-inline">
                                            <input name="enrolled" value="inCap" class="form-check-input" type="radio" id="inlineRadio1"
                                              value="option1">
                                            <label class="form-check-label" for="inlineRadio1">if your enrolled in CAP  </label>
                                          </div>
                                      
                                          <div class="form-check form-check-inline">
                                              <br>
                                            <input name="enrolled" value="notInCap" class="form-check-input" type="radio" id="inlineRadio2"
                                              value="option2">
                                            <label class="form-check-label" for="inlineRadio2">in university but not enrolled in CAP</label>
                                          </div>
                                          
                                          <div class="form-check form-check-inline">
                                              <br>
                                            <input name="enrolled" value="notInUni" class="form-check-input" type="radio" id="inlineRadio2"
                                              value="option2">
                                            <label class="form-check-label" for="inlineRadio2">not a university student yet</label>
                                            <br><br><br>
                                          </div>
                                          <!-- Submit success message-->
                                          <!---->
                                          <!-- This is what your users will see when the form-->
                                          
                                          <!---->
                                          <!-- This is what your users will see when there is-->
                                          <!-- an error submitting the form-->
                                          <div class="d-none" id="submitErrorMessage"><div class="text-center text-danger mb-3">Error sending message!</div></div>
                                          <!-- Submit Button-->
                                          <div class="d-grid">
                                          <button type="submit" class="btn btn-primary ">Submit</button><br>
                                          <div id="haveAccount" style=" text-align: center; ">
                                           <p>Already have an account? <a href="/login">Log in</a></p>
                                           <p> or </p>
                                            
                                                  <button class="btn btn-lg btn-block btn-primary" style="background-color: #ffffff; color: #dd4b39;"
                                                  type="submit"><img src="/images/google_icon.png"><a href ="/auth/google/signup">Sign up with google</a></button>
                                             </div>
                        
                                           </div>     
                                              <!--<button type="button" class="btn btn-link btn-floating mx-1">
                                                <i class="fa fa-google" aria-hidden="true"></i>
                                              </button>-->
                                      </form>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </section>
              </main>
             
  <%- include('./partials/footer') %>
      